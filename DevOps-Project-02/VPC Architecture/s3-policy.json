{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowReadAccessToSpecificS3Bucket",
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:ListBucket",
          "s3:GetBucketLocation"
        ],
        "Resource": [
          "arn:aws:s3:::s3-vpc-deployment",
          "aarn:aws:s3:::s3-vpc-deployment/*"
        ]
      }
    ]
  }
  